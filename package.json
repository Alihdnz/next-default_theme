{
  "name": "diniz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run scss:watch\"",
    "scss:watch": "sass resources/styles/main.scss public/css/styles.css --watch --style=compressed",
    "scss:build": "sass resources/styles/main.scss public/css/styles.css --style=compressed",
    "build": "next build && npm run scss:build"
  },
  "dependencies": {
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "sass": "^1.86.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
